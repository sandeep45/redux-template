{
  "name": "redux-template",
  "version": "1.0.0",
  "description": "```` mkdir redux-template cd redux-template npm init touch README.md ```` #### Load Dependencies",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "private": true,
  "scripts": {
    "start": "DEBUG=true X=true webpack-dev-server --debug --inline --hot --progress --colors --display-reasons",
    "test": "standard --verbose | snazzy && mocha --full-trace --growl --colors --bail --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:debug": "npm test -- debug",
    "dev_compile": "NODE_ENV=development webpack --progress --colors --display-error-details --display-chunks --display-reasons",
    "prod_compile": "NODE_ENV=production webpack -p -d --progress --colors --display-error-details --display-chunks --display-reasons --config webpack.production.config.js",
    "clean": "rm ./dist/bundle.js"
  },
